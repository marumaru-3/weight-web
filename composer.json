  "description": "体重管理アプリ",
  "require": {
    "php": "^8.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.5"
  },
  "autoload": {
    "psr-4": {
      "lib\\": "php/libs/"
    },
    "files": [
      "php/functions/helper.php",
      "php/functions/notice.php",
      "php/functions/validators.php"
